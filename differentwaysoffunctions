//different ways of function writing
 var add=(a,b)=>a+b;

 var add=(a,b)=>{return a+b;}

 var add=function(a,b){
    return a+b;
}


var result=add(12,13);
console.log(result);

//callback function

function intro()
{
    console.log("hi i am pulak");
}
var add=function(a,b,intro)
{
    var result=a+b;
    console.log("result is "+result);//main function work completed
    intro();
}
add(2,3,intro); // intro function is callback function kyoki ye main function add ke andar cll ho rha h 