var fs=require('fs');
var os=require('os');
var user=os.userInfo();
console.log(user);

//appednFil me pehla argument file ka path h fir data fir callback function
fs.appendFile("greetings.txt","hi "+user.username+"\n",()=>{console.log("file created")});


